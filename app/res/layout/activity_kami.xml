<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgmg"
    tools:context=".tentang">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="KELOMPOK 4"
        android:textSize="24sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="32dp" />

    <!-- Row 1 -->
    <ImageView
        android:id="@+id/image1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintTop_toBottomOf="@id/title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        android:layout_marginTop="60dp" />

    <TextView
        android:id="@+id/name1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jumria Purnamasari"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/image1"
        app:layout_constraintStart_toStartOf="@id/image1"
        app:layout_constraintEnd_toEndOf="@id/image1" />

    <TextView
        android:id="@+id/nim1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="202155202097"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/name1"
        app:layout_constraintStart_toStartOf="@id/name1"
        app:layout_constraintEnd_toEndOf="@id/name1" />

    <ImageView
        android:id="@+id/image2"
        android:layout_width="100dp"
        android:layout_height="100dp"

        app:layout_constraintTop_toBottomOf="@id/title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.7"
        android:layout_marginTop="60dp" />

    <TextView
        android:id="@+id/name2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dini"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/image2"
        app:layout_constraintStart_toStartOf="@id/image2"
        app:layout_constraintEnd_toEndOf="@id/image2" />

    <TextView
        android:id="@+id/nim2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="202155202111"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/name2"
        app:layout_constraintStart_toStartOf="@id/name2"
        app:layout_constraintEnd_toEndOf="@id/name2" />

    <!-- Row 2 -->
    <ImageView
        android:id="@+id/image3"
        android:layout_width="100dp"
        android:layout_height="100dp"

        app:layout_constraintTop_toBottomOf="@id/nim1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        android:layout_marginTop="40dp" />

    <TextView
        android:id="@+id/name3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sania"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/image3"
        app:layout_constraintStart_toStartOf="@id/image3"
        app:layout_constraintEnd_toEndOf="@id/image3" />

    <TextView
        android:id="@+id/nim3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="202155202020"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/name3"
        app:layout_constraintStart_toStartOf="@id/name3"
        app:layout_constraintEnd_toEndOf="@id/name3" />

    <ImageView
        android:id="@+id/image4"
        android:layout_width="100dp"
        android:layout_height="100dp"

        app:layout_constraintTop_toBottomOf="@id/nim2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.7"
        android:layout_marginTop="40dp" />

    <TextView
        android:id="@+id/name4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Atika"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/image4"
        app:layout_constraintStart_toStartOf="@id/image4"
        app:layout_constraintEnd_toEndOf="@id/image4" />

    <TextView
        android:id="@+id/nim4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="202155202064"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/name4"
        app:layout_constraintStart_toStartOf="@id/name4"
        app:layout_constraintEnd_toEndOf="@id/name4" />

    <!-- Row 3 -->
    <ImageView
        android:id="@+id/image5"
        android:layout_width="100dp"
        android:layout_height="100dp"

        app:layout_constraintTop_toBottomOf="@id/nim3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        android:layout_marginTop="40dp" />

    <TextView
        android:id="@+id/name5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Elsa"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/image5"
        app:layout_constraintStart_toStartOf="@id/image5"
        app:layout_constraintEnd_toEndOf="@id/image5" />

    <TextView
        android:id="@+id/nim5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="202155202111"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/name5"
        app:layout_constraintStart_toStartOf="@id/name5"
        app:layout_constraintEnd_toEndOf="@id/name5" />

    <ImageView
        android:id="@+id/image6"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintTop_toBottomOf="@id/nim4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.7"
        android:layout_marginTop="40dp" />

    <TextView
        android:id="@+id/name6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Permata"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/image6"
        app:layout_constraintStart_toStartOf="@id/image6"
        app:layout_constraintEnd_toEndOf="@id/image6" />

    <TextView
        android:id="@+id/nim6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="202155202089"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/name6"
        app:layout_constraintStart_toStartOf="@id/name6"
        app:layout_constraintEnd_toEndOf="@id/name6" />

</androidx.constraintlayout.widget.ConstraintLayout>
